{
    "name": "angwa/superban",
    "description": "This package helps to limit requests and ban suspicious users for a period of time",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "middleware", "security", "rate-limiting"],
    "homepage": "https://github.com/angwa/superban",
    "autoload": {
        "psr-4": {
            "Superban\\Tests\\": "tests/",
            "Superban\\": "src/"
        }
    },
    "require": {
        "php": "^8.0 || ^8.1",
        "laravel/framework": "^8.0|^9.0|^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.18"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
